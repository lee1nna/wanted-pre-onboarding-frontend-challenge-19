{
  "extends": ["react-app", "eslint:recommended", "airbnb"],
  "rules": {
    "no-var": "error",
    "no-multiple-empty-lines": "error",
    "no-console": ["error", {
      "allow": ["warn", "error", "info"]
    }],
    "eqeqeq": "error",
    "dot-notation": "error",
    "no-unused-vars": "error"
  }
}
